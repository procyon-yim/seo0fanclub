<% include header %>
  <% if (entries.length) { %>
    <% entries.forEach(function(entry) { %>
      <div class="panel panel-default">
        <div class="panel-heading">
          <div class="text-muted pull-right">
            <%= entry.published %>
          </div>
          <%= entry.title %>
        </div>
        <div class="panel-body">
          <%= entry.body %>
        </div>
      </div>
      <% }) %>
        <% } else { %> 보고 있는 당신, 어서 글을 남겨주세요! <a href="/new-entry">글 남기기</a>
          <% } %>
            <% include footer %>